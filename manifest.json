{
    "name": "NBA Standings",
    "description": "Displays NBA standings for the current season",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "https://api-basketball.p.rapidapi.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "script.js"
        ]
      }
    ]
  }